digraph "Somente um zero no inicio e no fim" {
	graph [rankdir=LR]
	node [shape=circle]
	edge [arrowhead=vee arrowsize=1]
	Start [label="" shape=none]
	T
	A
	C
	B
	D [shape=doublecircle]
	Start -> A
	A -> T [label="11R"]
	A -> B [label="0XR"]
	B -> B [label="1YR"]
	B -> T [label="**R"]
	B -> C [label="0XR"]
	C -> T [label="11R"]
	C -> T [label="00R"]
	C -> D [label="**R"]
}
